<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/group4"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="16dp"
    app:cardBackgroundColor="@color/profile_card_background"
    app:cardCornerRadius="16dp"
    app:cardElevation="0dp"
    app:layout_constraintTop_toBottomOf="@id/group3">

    <com.flowfoundation.wallet.page.profile.widget.ProfilePreference
        android:id="@+id/switch_account_preference"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:icon="@drawable/ic_settings_switch_account"
        app:titleId="@string/switch_profile"/>


</com.google.android.material.card.MaterialCardView>